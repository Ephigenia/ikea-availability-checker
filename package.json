{
  "author": "Ephigenia M. Eichner",
  "name": "ikea-availability-checker",
  "description": "ikea product in-store availability checker and product search",
  "version": "2.0.0-alpha.3",
  "preferGlobal": true,
  "keywords": [
    "availability",
    "furniture",
    "ikea",
    "iows",
    "ingka",
    "product",
    "cli",
    "command-line-tool",
    "commandline",
    "restock",
    "search",
    "stock",
    "warehouse"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "maintainer": {
    "name": "Marcel Eichner",
    "email": "marcel.eichner@ephigenia.de"
  },
  "contributors": [
    {
      "name": "Marcel Eichner",
      "email": "marcel.eichner@ephigenia.de"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ephigenia/ikea-availability-checker.git"
  },
  "funding": {
    "url": "http://github.com/sponsors/Ephigenia"
  },
  "bugs": {
    "url": "https://github.com/Ephigenia/ikea-availability-checker/issues"
  },
  "files": [
    "source",
    "!source/**/*.test.js",
    "!source/test",
    "!.DS_Store",
    "types",
    "!types/**/*.test-d.ts"
  ],
  "main": "source/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "ikea-availability": "source/cli.js"
  },
  "scripts": {
    "lint": "eslint source/**/*.js",
    "lint:fix": "npm run -s lint -- --fix",
    "lint:types": "tsd",
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "node source/cli.js",
    "tdd": "npm run -s test -- --watch",
    "test": "NODE_ENV=test nyc --reporter=text-summary --reporter=lcov mocha \"source/**/*.test.js\"",
    "test:ci": "npm run -s test",
    "semantic-release": "semantic-release",
    "typings": "tsc"
  },
  "dependencies": {
    "axios": "0.26.1",
    "chalk": "4.1.2",
    "cli-table3": "0.6.1",
    "commander": "9.1.0",
    "i18n-iso-countries": "7.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-angular": "16.2.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/node": "17.0.23",
    "chai": "4.3.6",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "mocha": "9.2.2",
    "nock": "13.2.4",
    "nyc": "15.1.0",
    "pinst": "3.0.0",
    "semantic-release": "19.0.2",
    "sinon": "13.0.1",
    "tsd": "^0.19.1",
    "typescript": "4.6.3"
  }
}
